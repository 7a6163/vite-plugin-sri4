{
  "name": "vite-plugin-sri4",
  "version": "1.1.0",
  "description": "A Vite plugin to generate Subresource Integrity (SRI) hashes for output files.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "node-fetch": "^3.3.0"
  },
  "peerDependencies": {
    "vite": "^4.0.0 || ^5.0.0 || ^6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@vitest/coverage-v8": "^3.0.5",
    "rollup": "^4.0.0",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "keywords": [
    "vite",
    "plugin",
    "sri",
    "subresource integrity"
  ],
  "author": "Zac",
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/7a6163/vite-plugin-sri4.git"
  },
  "bugs": {
    "url": "https://github.com/7a6163/vite-plugin-sri4/issues"
  },
  "homepage": "https://github.com/7a6163/vite-plugin-sri4#readme"
}
